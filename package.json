{
  "name": "react-hmp-templete",
  "version": "1.0.0",
  "private": true,
  "author": "",
  "description": "",
  "keywords": [],
  "scripts": {
    "start": "react-app-rewired start",
    "mocker": "react-app-rewired start mocker=true",
    "build": "react-app-rewired build",
    "build:test": "react-app-rewired build env=test",
    "build:release": "react-app-rewired build env=release",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "npm run lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "analyze": "source-map-explorer 'dist/static/js/*.js'"
  },
  "husky": {

    "hooks": {
      "pre-commit":"lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
    
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
